        "@testing-library/jest-dom": "^5.9.0",
        "@testing-library/react": "^10.2.0",
        "axios": "^0.19",
        "babel-jest": "^26.0.1",
        "bootstrap": "^4.0.0",
        "cross-env": "^7.0",
        "jest": "^26.0.1",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
